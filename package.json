{
  "name": "harvard-houses",
  "description": "Get Harvard house names",
  "version": "0.0.0-semantically-released",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm-run-all --parallel build:*",
    "build:main": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "build:umd": "webpack --output-filename index.umd.js",
    "build:umd.min": "webpack --output-filename index.umd.min.js -p",
    "commit": "git-cz",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm test -- -watch",
    "test": "mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tobiasbueschel/harvard-houses.git"
  },
  "keywords": [
    "harvard"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Tobias BÃ¼schel <tobias.buschel.15@ucl.ac.uk>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tobiasbueschel/harvard-houses/issues"
  },
  "homepage": "https://github.com/tobiasbueschel/harvard-houses#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.11.4",
    "babel-loader": "6.2.4",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-stage-2": "6.11.0",
    "babel-register": "6.11.6",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.2",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.3.2",
    "istanbul": "0.4.4",
    "json-loader": "0.5.4",
    "mocha": "3.0.0",
    "npm-run-all": "2.3.0",
    "nyc": "7.1.0",
    "rimraf": "2.5.4",
    "semantic-release": "^4.3.5",
    "webpack": "1.13.1"
    "standard": "^10.0.1",
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  }
}
